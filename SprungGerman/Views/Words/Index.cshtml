@model SprungGerman.Models.WordsListViewModel

@{
    ViewData["Title"] = "Words";
}

<title>Title</title>
<link href="~/css/progress/wordsLearned.css" rel="stylesheet" />

<div id="words">

    <h3> Words I know by now</h3>

    <table id="wordsList">

        <thead>
            <tr>
                <th>Id -></th>
                <th>English <<->> </th>
                <th>German </th>
            </tr>
        </thead>

        <tbody>

            @foreach (var words in Model.Words)
            {

                <tr>
                    <td>@words.Id</td>
                    <td>@words.EnglishVersion</td>
                    <td>@words.GermanVersion</td>
                </tr>
            }

        </tbody>
    </table>

    

</div>


